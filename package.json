{
  "name": "use-route-matching",
  "version": "0.0.0-alpha.7",
  "description": "use route matching hook",
  "files": [
    "dist",
    "lib",
    "index.js",
    "index.d.ts",
    "use-route-matching.hook.js",
    "LICENSE.txt",
    "package.json",
    "readme.md"
  ],
  "browser": "index.js",
  "main": "index.js",
  "module": "lib/index.esm.js",
  "types": "index.d.ts",
  "jsnext:main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v-borrego/use-route-matching.git"
  },
  "keywords": [
    "react",
    "hook",
    "route",
    "match",
    "matching",
    "routes",
    "location",
    "url",
    "params",
    "history"
  ],
  "author": "Victor Borrego Perez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/v-borrego/use-route-matching/issues"
  },
  "homepage": "https://github.com/v-borrego/use-route-matching#readme",
  "scripts": {
    "clean": "rimraf build/*",
    "clean:demo": "rimraf build-demo/*",
    "typecheck": "tsc --pretty --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "build": "npm run clean && npm run build:lib && npm run build:lib:esm && npm run build:umd && npm run build:es && npm run build:umd:min && npm run build:copyfiles && npm run build:copydts",
    "build:lib": "cross-env BABEL_ENV=es5_cjs babel ./src --out-dir ./build/lib --ignore 'src/**/*.test.ts,src/**/*.test.js,src/**/*.d.ts' --extensions '.ts,.tsx,.js'",
    "build:lib:esm": "cross-env BABEL_ENV=es5_esm babel ./src/index.ts --out-file ./build/lib/index.esm.js",
    "build:es": "cross-env BABEL_ENV=es babel --config-file ./.babelrc ./src --out-dir ./build --ignore 'src/**/*.test.ts,src/**/*.test.js,src/**/*.d.ts' --extensions '.ts,.tsx,.js'",
    "build:umd": "cross-env BABEL_ENV=umd webpack --config ./config/webpack/webpack.prod.js",
    "build:umd:min": "cross-env BABEL_ENV=umd webpack -p --config ./config/webpack/webpack.prod.js",
    "build:copyfiles": "copyfiles package.json index.d.ts readme.md LICENSE.txt silent-renew.html build",
    "build:copydts": "copyfiles \"./src/**/*.d.ts\" -u 1 build",
    "start:demo": "cross-env BABEL_ENV=umd webpack-dev-server --port 4556 --config ./config/webpack/webpack.demo.js --open",
    "test": "cross-env NODE_ENV=test jest --verbose --config jest.json --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --verbose --watchAll --config jest.json",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --rule \"react-hooks/exhaustive-deps:off\"",
    "lint:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" && npm run lint -- --fix",
    "release": "npm publish ./build",
    "release:local": "npm install ./build -g"
  },
  "dependencies": {
    "core-js": "^3.0.1"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "react-router-dom": ">=5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/history": "^4.7.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.12.10",
    "@types/react": "^16.8.1",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.2",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "prettier": "^1.14.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.1.2",
    "rimraf": "^2.6.2",
    "to-camel-case": "^1.0.0",
    "ts-jest": "^23.10.1",
    "typescript": "^3.0.3",
    "url-loader": "^1.1.2",
    "webpack": "4.28.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10"
  }
}
